<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FormView">
        <attr name="Xname" format="string"/>
        <attr name="XLineColor" format="color|integer"/>
        <attr name="XNameColor" format="color|integer"/>
        <attr name="XNameSize" format="color|integer"/>

        <attr name="Yname" format="string"/>
